# GPT-OSS MixLoRA Environment Variables
# =====================================
# Copy this file to .env and fill in your values
# Usage: cp .env.example .env

# =============================================================================
# REQUIRED - HuggingFace Token
# =============================================================================
# Get your token from: https://huggingface.co/settings/tokens
HF_TOKEN=hf_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# =============================================================================
# OPTIONAL - Weights & Biases (for logging)
# =============================================================================
# Get your key from: https://wandb.ai/settings
WANDB_API_KEY=
WANDB_PROJECT=mixlora-gptoss
WANDB_ENTITY=

# =============================================================================
# OPTIONAL - HuggingFace Repo Settings
# =============================================================================
HF_USERNAME=twanghcmut
HF_REPO_NAME=mixlora-gpt-oss-experimental

# =============================================================================
# OPTIONAL - Model Settings
# =============================================================================
MODEL_ID=openai/gpt-oss-20b

# =============================================================================
# OPTIONAL - CUDA Settings
# =============================================================================
CUDA_VISIBLE_DEVICES=0
